{
	"name": "vscode-nls",
	"version": "5.2.0",
	"description": "NPM module to externalize and localize VSCode extensions",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-nls.git"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-nls/issues"
	},
	"main": "./lib/node/main.js",
	"browser": {
		"./lib/node/main.js": "./lib/browser/main.js"
	},
	"typings": "./lib/common/common.d.ts",
	"devDependencies": {
		"@types/mocha": "10.0.6",
		"@types/node": "20.14.5",
		"@typescript-eslint/parser": "7.13.1",
		"eslint": "9.5.0",
		"mocha": "10.4.0",
		"rimraf": "5.0.7",
		"typescript": "5.4.5"
	},
	"scripts": {
		"prepublishOnly": "npm run clean && npm run compile && npm run lint && npm run test",
		"compile": "tsc -b ./tsconfig.json",
		"watch": "tsc -b ./tsconfig.json -w",
		"clean": "rimraf lib",
		"test": "mocha",
		"lint": "eslint --config .eslintrc.json ./**/*.ts"
	},
	"files": [
		"lib/common",
		"lib/browser",
		"lib/node",
		"!lib/*/test",
		"!lib/**/*.tsbuildinfo",
		"node.js",
		"node.d.ts",
		"browser.js",
		"browser.d.ts"
	]
}
