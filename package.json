{
	"name": "vscode-nls",
	"version": "0.0.1",
	"private": false,
	"description": "NPM module to externalize and localize VSCode extensions",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./lib/node/main.js",
	"browser": {
		"./lib/node/main.js": "./lib/browser/main.js"
	},
	"typings": "./lib/common/common.d.ts",
	"files": [
		"lib/common",
		"lib/browser",
		"lib/node",
		"!lib/*/test",
		"!lib/**/*.tsbuildinfo",
		"node.js",
		"node.d.ts",
		"browser.js",
		"browser.d.ts"
	],
	"scripts": {
		"clean": "rimraf lib",
		"compile": "tsc -b ./tsconfig.json",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "tsc -b ./tsconfig.json -w"
	},
	"devDependencies": {
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.30",
		"mocha": "10.3.0",
		"rimraf": "5.0.5",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
